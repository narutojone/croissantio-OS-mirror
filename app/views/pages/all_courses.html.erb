<%= content_for :css do %>
  <%= stylesheet_link_tag  "bootstrap", "models/articles",  "stack-interface", "socicon", "lightbox.min", "flickity", "iconsmind", "theme", "models/pages"  %>
  <link href="https://fonts.googleapis.com/css?family=Open+Sans:200,300,400,400i,500,600,700%7CMerriweather:300,300i%7CMaterial+Icons" rel="stylesheet">
<% end %>
<%= render "shared/stack_navbar" %>
<div class="main-container">
  <section class="text-center">
		<div class="container">
				<div class="row">
					<div class="col-sm-10 col-sm-offset-1 col-md-12">
						<h1>List of all courses:</h1>
						<ul>
							<li><%= link_to "Marketing 101 for B2B SaaS", marketing_course_path %></li>
						</ul>
			</div>
			</div>
	</section>
</div>
<% content_for :js do %>
<%= javascript_include_tag "flickity.min", "parallax", "typed.min", "isotope.min", "ytplayer.min", "lightbox.min", "granim.min", "countdown.min", "twitterfetcher.min", "spectragram.min", "smooth-scroll.min", "script", "scripts"  %>
<script type="text/javascript">
  function Subscribed() {
  	analytics.track('Subscribed');
  	window.optimizely.push(["trackEvent", "Subscribed"]);
  }

  analytics.page('All Courses');

</script>
<% end %>